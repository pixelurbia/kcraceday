  

//COLORS
$white: #fff;
$darkgrey: #333;
$lightgray:#e7e0de;
$grey:#a6a6a5;
$black: #000;
$rkcorange:#f4945c;
$btnblue:#5db5da;
$rkcgreen:#546059;
$kcrdarkgreen:#546059;
$offwhite:#e7dbd8;
$kcrblue:#5db5da;
$bluehover:#4ba2c7;


//IMPORT
@import 'bourbon/bourbon';
@import 'neat/neat';
@import 'jkr/jkr';

//FONTS

$font-stack: "proxima-nova",sans-serif;

@mixin display {
	font-family: "proxima-nova",sans-serif;
}
@mixin paragraph {
	font-family: "proxima-nova",sans-serif;
}

html {  }
body { font-family: $font-stack ; height:auto; font-size:14px; display:none; background: $rkcorange url('../images/bodybg.png') center center no-repeat fixed;
  -webkit-background-size: cover;
  -moz-background-size: cover;
  -o-background-size: cover;
  -webkit-backface-visibility: visible!important;
  background-size: cover;  

 @include paragraph; font-size: 14px; font-size: 1.4rem; -webkit-backface-visibility: hidden;

}

// Media Querie sizes  

$phone: 400px;
$ipad: 768px;

// @include media($phone) {}
// @include media($ipad) {}

// Type
h1, h2, h3, h4, h5 {margin:5px 0; margin-bottom:5px; padding:0; text-decoration: none; font-family: $font-stack ;}
h1, h1 a { font-size:30px;  color:#f4945c; font-weight:500; text-decoration:none;}
h2, h2 a { font-size:26px; color:#728379; font-weight:200;}
h3, h3 a { font-size:20px; color:$darkgrey;}
h4, h4 a { font-size:18px; color:$offwhite; font-weight:500;}
h5, h5 a { font-size:16px; color:$offwhite; font-weight:100;}
h6, h6 a { font-size:14px; color:#546059; font-weight: 700;}

p { color: $rkcgreen; line-height: 1.6em; margin: 5px 0; font-size:14px; }
a { color: $kcrblue;
	&:hover { text-decoration: none; }
	&:focus { outline: 0; }
}


// Utility
.clearfix { @include clearfix; }
.clear { clear: both; }
.left { float: left; }
.right { float: right; }
hr { clear: both; width: 100%; height: 1px; background: #bbb; border: 0; }
img { width:100%; height:auto; }
.hidden {display:none;}

//global styles

.wrap { margin:0 auto; max-width:1000px;}
.contentWrap { margin: 0 auto; max-width: 960px; height: auto; display: block; overflow:hidden; } 
.btn { font-size: 14px; cursor: pointer; background: $btnblue; color: #fff; border-radius: 5px; padding: 10px; font-weight: 700; text-decoration: none; text-transform: uppercase; line-height: 22px; display: block; max-width: 190px; text-align: center; -moz-box-shadow: 1px 1px 0px 1px #3c819d; -webkit-box-shadow: 1px 1px 0px 1px #3c819d; box-shadow: 1px 1px 0px 1px #3c819d;
	&:hover{background: #3c819d; transition: background 1s;  }
	}

.ui-datepicker-trigger {display:none;}

//Global Parts
.blured {background:url('../images/transparent.png');}

.header { width:100%; max-width:177px; height:auto; float:left; 
  @media screen and (max-width: $phone) { width:95%; max-width:400px; margin:0 auto; float:none;}
  .parts {height: 378px; width: 178px; position: absolute; display: block; top:0;}
  .contain {position:relative; z-index:99;}
  .topnav { 
    .menu {height:60px; font-size:12px;
        @media screen and (max-width: $phone) { font-size:14px;}
    li {float:left; width: 33%; text-align: center; padding-top:10px;font-weight:100;
      &:hover{ border-top:solid 3px $rkcorange; transition: border-top 0.5s}}
    li a { color:#fff; text-decoration:none; height:10px;}}}
  .logo { max-width:150px; height:auto; 
    @media screen and (max-width: $phone) { display:none;} 
  img { width:100%; height:auto; padding:0px 15px 15px 15px; opacity:1; -webkit-transition: opacity 1s;
    &:hover{ transition: opacity 1s; -webkit-transition: opacity 1s; /* Safari */ opacity:0.7;}}}
    .logo.mobile {display:none;
      @media screen and (max-width: $phone) { display:block;  max-width:100%; height:auto; }
       img { width:100%; height:auto; padding:0px 15px 15px 15px; opacity:1; -webkit-transition: opacity 1s;
    &:hover{ transition: opacity 1s; -webkit-transition: opacity 1s; /* Safari */ opacity:0.7;}}}
  .mainnav { 
    .menu {
    li {height:46px; display: block; line-height: 45px; text-indent: 8px; border-bottom:solid 1px #fff;  -webkit-transition: background 1s;
      &:hover{ transition: background 1s; -webkit-transition: background 1s; /* Safari */ background:$rkcorange; }}
      .menu-item-100 a {background-position:146px -30px;}
      .menu-item-26 a {background-position:146px -75px;}
      .menu-item-18 a {background-position:146px -120px;}
    li a { font-size:16px; color:#fff; text-decoration:none; font-family:$font-stack;display: block; width: 100%; background: url('../images/navicons.png')  146px 15px no-repeat;
    @media screen and (max-width: $phone) {background:none; text-align:center;} }
  }}

.searchform { position: absolute; width: 138px; margin-top: -22px; display:none; 
  label {display:none;}
  input {-webkit-appearance: none; background: url("../images/navicons.png") #3C3C3C 11px -129px no-repeat; text-indent: 7px; border: none; width: 179px; height: 26px; overflow: hidden; padding-left: 32px; margin-top: 6px; font-size: 16px; color: $white; text-decoration: none; font-family: "proxima-nova", sans-serif;}
}
#searchsubmit {display:none;}

.mobileNav {display:none;}
}

//homepage

.contentarea {margin-top:52px; max-width:565px; width:100%; padding:0px 10px 0px 10px; float:left; display:block;
 .btn.alertmobile { display:none; text-transform:none!important;
@media screen and (max-width: $ipad) {display:block;  max-width: 100%; font-weight: 700; background: #f4945c; -moz-box-shadow: 1px 1px 0px 1px #3c819d; -webkit-box-shadow: 1px 1px 0px 1px #BD512F; box-shadow: 1px 1px 0px 1px #BD512F; }
@media screen and (max-width: $phone) {display:block;  max-width: 100%; font-weight: 700; background: #f4945c; -moz-box-shadow: 1px 1px 0px 1px #3c819d; -webkit-box-shadow: 1px 1px 0px 1px #BD512F; box-shadow: 1px 1px 0px 1px #BD512F; }
    } 
  .toparea { height:10px; width:100%;}
  .headers {background:$white; padding: 10px 40px 10px 40px; text-align:center; }
  .thecontent {background:#faf2f0; height:auto; line-height:18px; padding: 10px 12px 10px 12px;overflow: hidden; }
  .botarea { height:10px; width:100%; margin-bottom:80px;        }
  .btn {margin:0 auto;}
  p {padding-right: 25px;}
  .reqp { float: left; width: 280px; position: absolute;
    @media screen and (max-width: $phone) {float: left; width: 280px; position: relative;} }
    
  li ul li {font-size: 14px; list-style: disc; margin-left: 30px; }
}

.contentarea.fullwidth {max-width: 823px; 
  .col-1-2 {width: 65%; margin-left: 35px;
      @media screen and (max-width: $phone) { margin-left:0px; }}
  .dwnbtn {background:url('../images/downloadbtn.png') 0px 0px no-repeat; font-size: 14px; padding: 0px 5px 5px 20px;}
  h3 {margin: 20px 0px 20px 0px;}
}

.apply {
  .btn {max-width:100%; margin-top:38px; margin-bottom:15px; text-align: left; text-transform: none; font-weight: 100;}
  .orangearea {background: $rkcorange; padding:10px; display:block; color:$white; font-size:14px;
    }
    h2 {font-size:20px; color:$white;}
}

//Mobile
#phonedummy {
  display: none; height: 0; width: 0; //hide
    @media screen and (max-width: $phone) { display:block; } //show if mobile 
}




//sidebars
.sidebar { margin-top:14px; margin-bottom: 80px; width:245px; display:block; float:left; overflow: hidden; 
