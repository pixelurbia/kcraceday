//Application Calendar
 .cal-c.calapply.loading {opacity: 0.3;-webkit-transition: opacity 1s; } //for loading the ajax call
.cal-c.calapply { width:100%; max-width: 523px; height: auto; overflow: hidden; padding-left: 0px; margin-bottom:0px; opacity:1;  -webkit-transition: opacity 1s;
   @media screen and (max-width: $phone) {max-width: 275px; display:block;}
  .scrolled {overflow:hidden;} 
    //for date value saving and date swtiching 
    .adate i, .adate p {display:none;}
    #fieldPrev, #fieldNext { margin: 6px; width: 33px; height: 33px;}
    #fieldnext {background-position: -45px 0px;}
