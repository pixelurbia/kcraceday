      &:hover { color:$kcrblue; background:none; text-decoration:none; }}}
.form { color:$white; font-size:14px;
  h4 { font-weight:600; margin-left:0px; margin-bottom:0px;}
  h5 { font-weight:400;}
  p, a {color:$white;}
  p {margin-top:0px; margin-bottom:0px}
  a {margin:0;}
  .btn { margin: 0 auto; margin-top:30px; }  
  input[type=submit] {margin: 12px auto; font-size: 14px; cursor: pointer; background: $btnblue; border:0px; color: #fff; border-radius: 5px; padding: 10px; font-weight: 700; text-decoration: none; text-transform: uppercase; line-height: 22px; display: block; max-width: 190px; text-align: center; -moz-box-shadow: 1px 1px 0px 1px #3c819d; -webkit-box-shadow: 1px 1px 0px 1px #3c819d; box-shadow: 1px 1px 0px 1px #3c819d;
  &:hover{background: #3c819d; transition: background 1s;  -webkit-appearance: none; } }
  label {display:none;}
input { width:100%; height:35px!important; background:#fff; color:$darkgrey; display:block; line-height:35px; padding-left:12px; border:solid 1px #eee; margin:8px; margin-left:0px; -webkit-appearance: none; }
select { color: #B3B3B3; overflow: hidden; width: 228px; border-radius:0px; padding-right: 44px; background: $white url('../images/formarrow.png') 197px 9px no-repeat; -webkit-appearance: none; -moz-appearance:none; appearance:none; width:100%; height:35px;  display:block; line-height:35px; padding-left:12px; border:solid 1px #eee; margin:5px; margin-left:0px; text-indent:1px; text-overflow:'';
  @media screen and (max-width: $phone) { background-position:242px 9px; } 
  @media screen and (max-width: $ipad) { background-position:575px 9px; } } 
select::-ms-expand {display:none;}}
}
.sidebar.apply {  
  @media screen and (max-width: $phone) { display:none; }
.btn {line-height:15px;
h2 {line-height:22px;}}}
.sidebar.mobile { display:none;
    @media screen and (max-width: $phone) { display:block; margin-bottom: 80px;}
    @media screen and (max-width: $ipad) { display:block; margin-bottom: 80px;}}

//Columns 
.col-1-3 { width:30%; float:left; display:block;
