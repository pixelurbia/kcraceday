    //styles
    select {float: left; width: 116px; font-size: 16px; border-radius: 0px; height: 28px; background: #fff; color: #333333; display: block; line-height: 24px; padding-left: 12px; border: solid 0px #eee; margin: 5px; margin-left: 0px; background: none; -webkit-appearance: none; -moz-appearance: none; appearance: none; text-indent: 1px; text-overflow: '';}
      select::-ms-expand {display:none;}
    #year-select, #month-select { display:none;}
    .cal-controls {margin-top:2px; margin-bottom:25px;
    @media screen and (max-width: $phone) { display:block; }}
  .cal-view { float: right; padding-top: 15px;
    li.calactive { width: 105px; float:left;  display: block; height: 23px; overflow: hidden; cursor:pointer;  color:$white; background: url('../images/calview.png') 082px 4px no-repeat; font-size: 14px; font-weight:400; line-height: 23px; text-indent: 10px; margin-right:10px; -webkit-transition: background 1s;
      &:hover {background:$offwhite url('../images/calview.png') 082px 4px no-repeat; -webkit-transition: background 1s;}
    }   
    li {  width: 105px; float:left;  display: block; height: 23px; overflow: hidden; cursor:pointer;  color:$white; background:$kcrblue  url('../images/listview.png') 082px 4px no-repeat; font-size: 14px; font-weight:400; line-height: 23px; text-indent: 10px; margin-right:10px; -webkit-transition: background 1s;
      &:hover {background:$bluehover url('../images/listview.png') 082px 4px no-repeat; -webkit-transition: background 1s;} 
      a { color:$white; text-decoration:none; }
    } 
    .activeview {background-color:transparent !important; -webkit-transition: background 1s; 
      &:hover {background-color:$bluehover !important; -webkit-transition: background 1s;}
    }
