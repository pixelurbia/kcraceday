  @media screen and (max-width: $ipad) { display:block!important; width:85%; margin:0 auto; float:none; overflow:hidden; margin-bottom:100px; height:auto;}
.upe { margin-bottom:13px;
  @media screen and (max-width: $phone) { display:none; }
  @media screen and (max-width: $ipad) { display:none; }}
  .contain {position:relative; z-index:99;}
  h4 {font-weight:500; margin-left:15px; margin-bottom:15px;}
  .toparea { height:10px; width:100%;}
  h5 { margin-bottom:30px;}
  img {margin-bottom: -5px;}
  .sbcontent { padding:9px;
